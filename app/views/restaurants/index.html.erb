<h1>Restaurants</h1>

<ul>
  <% @restaurants.each do |restaurant| %>
    <li>
      <%= link_to "like", toggle_favorite_restaurant_path(restaurant), remote: true, method: :post %>
      <%= restaurant.name %>
      <%= link_to "Delete", restaurant, method: :delete, remote: true %>
    </li>
  <% end %>
</ul>
